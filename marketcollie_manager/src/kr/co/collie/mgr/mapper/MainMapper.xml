<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.collie.mgr.main">
	<select id="selectMgrId" parameterType="kr.co.collie.mgr.main.vo.MgrLoginVO" resultType="String">
		select id
		from admin
		where id=#{id} and pass=#{pass}
	</select>
</mapper>